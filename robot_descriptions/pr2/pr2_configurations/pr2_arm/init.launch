<launch>
  <param name="robotdesc/pr2" command="$(find xacro)/xacro.py '$(find pr2_defs)/robots/r_arm.xacro.xml'" />
  <include file="$(find pr2_arm)/calibrate.launch" />
  <group name="wg">
    <node pkg="pr2_etherCAT" type="pr2_etherCAT" args="-i rteth0 -x /robotdesc/pr2"
          output="screen"/>
  </group>
</launch>

