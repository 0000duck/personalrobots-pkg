
<launch>
<param name="people_tracker/freq" value="10.0"/>

<param name="people_tracker/sys_sigma_pos_x" value="0.1"/>
<param name="people_tracker/sys_sigma_pos_y" value="0.1"/>
<param name="people_tracker/sys_sigma_pos_z" value="0.0000001"/>
<param name="people_tracker/sys_sigma_vel_x" value="0.1"/>
<param name="people_tracker/sys_sigma_vel_y" value="0.1"/>
<param name="people_tracker/sys_sigma_vel_z" value="0.0000001"/>

<param name="people_tracker/prior_sigma_pos_x" value="0.3"/>
<param name="people_tracker/prior_sigma_pos_y" value="0.3"/>
<param name="people_tracker/prior_sigma_pos_z" value="0.000001"/>
<param name="people_tracker/prior_sigma_vel_x" value="0.000001"/>
<param name="people_tracker/prior_sigma_vel_y" value="0.000001"/>
<param name="people_tracker/prior_sigma_vel_z" value="0.000001"/>

<node pkg="people" type="people_tracker" output="screen"/>
</launch>

