<!-- Launch this file, then turn on the visualization_marker and tm_driver/connectors topics in rviz, to see the topological map -->
<launch>
  <master auto="start"/>
  <group name="wg">
    <node pkg="tf" type="transform_sender" args="0 0 0 0 0 0 map none 10"/>
    <node pkg="topological_map" type="visualize" args="-t $(find willow_maps)/willow.tmap -d $(find willow_maps)/willow.tmap.door_overrides.xml" respawn="false" output="screen" />
  </group>
</launch>
