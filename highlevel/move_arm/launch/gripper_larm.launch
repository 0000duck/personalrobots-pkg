<launch>

  <!-- Gripper effort controller -->
  <node pkg="mechanism_control" type="spawner.py" args="$(find pr2_default_controllers)/l_gripper_effort_controller.xml" />

  <node pkg="move_arm" type="actuate_gripper_action">
    <remap from="gripper_command" to="l_gripper_effort_controller/command" />
    <param name="arm" type="string" value="left_arm" />
  </node>

</launch>
