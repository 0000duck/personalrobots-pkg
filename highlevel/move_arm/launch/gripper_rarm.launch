<launch>

  <!-- Gripper effort controller -->
  <rosparam command="load" file="$(find pr2_default_controllers)/pr2_joint_effort_controllers.yaml" />
  <node pkg="pr2_mechanism_control" type="spawner.py" args="r_gripper_effort_controller" />

  <node pkg="move_arm" type="actuate_gripper_action" name="actuate_gripper_right_arm">
    <remap from="gripper_command" to="r_gripper_effort_controller/command" />
    <param name="arm" type="string" value="right_arm" />
  </node>

</launch>
