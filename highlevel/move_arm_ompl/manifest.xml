<package>
  <description brief="move_arm_ompl">

    Given an arm goal, attempt to reach it. This is done using OMPL
    and IK. Replanning is performed if changes are observed in the
    environment.

  </description>
  <author>Ioan Sucan</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://pr.willowgarage.com/wiki/move_arm_ompl</url>

  <depend package="roscpp"/>
  <depend package="rosconsole"/>
  <depend package="std_msgs"/>
  <depend package="robot_msgs"/>
  <depend package="visualization_msgs"/>
  <depend package="motion_planning_msgs"/>
  <depend package="motion_planning_srvs"/>
  <depend package="robot_actions"/>
  <depend package="ompl_planning"/>
  <depend package="pr2_ik"/>

</package>


