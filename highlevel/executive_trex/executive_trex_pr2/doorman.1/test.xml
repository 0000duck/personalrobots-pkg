<launch>
  <master auto="start"/>
  <node pkg="executive_trex_pr2" type="stub_ros_container" respawn="false" />

  <param name="/trex/input_file" value="m2.cfg"/>
  <param name="/trex/path" value="$(find executive_trex_pr2)/cfg:$(find executive_trex_pr2)/doorman.1:$(find executive_trex_pr2)/cfg"/>
  <param name="/trex/time_limit" value="40"/>
  <param name="/trex/log_dir" value="$(find executive_trex_pr2)/doorman.1"/>
  <param name="/trex/play_back" value="0"/>
  <test test-name="executive_trex_doorman.1" pkg="executive_trex_pr2" type="trexfast" time-limit="50"/>
</launch>
