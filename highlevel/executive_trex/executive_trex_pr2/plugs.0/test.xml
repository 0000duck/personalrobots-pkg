<launch>
  <master auto="start"/>
  <param name="/trex/input_file" value="m2.recharger.sim.cfg"/>
  <param name="/trex/path" value="$(find executive_trex_pr2)/cfg:$(find executive_trex_pr2)/plugs.0:$(find executive_trex_pr2)/cfg"/>
  <param name="/trex/time_limit" value="40"/>
  <param name="/trex/log_dir" value="$(find executive_trex_pr2)/plugs.0"/>
  <param name="/trex/play_back" value="0"/>
  <test test-name="executive_trex_plugs.0" pkg="executive_trex_pr2" type="trexfast" time-limit="60"/>
</launch>
