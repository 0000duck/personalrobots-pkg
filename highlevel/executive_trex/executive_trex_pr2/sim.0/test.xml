<launch>
  <master auto="start"/>
  <param name="/trex/input_file" value="sim.cfg"/>
  <param name="/trex/path" value="$(find executive_trex_pr2)/cfg:$(find executive_trex_pr2)/sim.0"/>
  <param name="/trex/time_limit" value="20"/>
  <param name="/trex/log_dir" value="$(find executive_trex_pr2)/sim.0"/>
  <param name="/trex/play_back" value="0"/>
  <test test-name="executive_trex_sim.0" pkg="executive_trex_pr2" type="trexdebug" time-limit="40"/>
</launch>
