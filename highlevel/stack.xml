<stack name="highlevel" version="0.1">
  <description brief="high-level packages from ros-pkg">
   This stack is meant to contain high-level stuff, i.e., software
   that views the robot in very abstract terms, like "drive to XY on a
   map" or "open the door in front of you" or "pick up the stapler in
   front of you."
  </description>
  <author>Conor McGann mcgann@willowgarage.com</author>
  <license>BSD</license>
  <review status="unreviewed" notes=""/>
  <url>http://pr.willowgarage.com/wiki/highlevel</url>
  
  <depend stack="pr2_mechanism"/> <!-- pr2_mechanism_msgs -->
  <depend stack="visual_feature_detectors"/> <!-- outlet_detection -->
  <depend stack="geometry"/> <!-- kdl, tf, tf_conversions -->
  <depend stack="semantic_mapping"/> <!-- plug_onbase_detector, door_handle_detector, point_cloud_mapping -->
  <depend stack="visualization_common"/> <!-- visualization_msgs -->
  <depend stack="motion_planning"/> <!-- planning_models, planning_environment, motion_planning_msgs -->
  <depend stack="controllers"/> <!-- pr2_mechanism_controllers, robot_mechanism_controllers -->
  <depend stack="laser_pipeline"/> <!-- point_cloud_assembler -->
  <depend stack="sandbox"/> <!-- pr2_robot_actions, manipulation_srvs, move_base_client, mapping_msgs, experimental_controllers, manipulation_msgs -->
  <depend stack="common"/> <!-- actionlib -->
  <depend stack="simulator_stage"/> <!-- stage -->
  <depend stack="diagnostics"/> <!-- diagnostic_updater -->
  <depend stack="pr2_common"/> <!-- pr2_msgs -->
  <depend stack="ros"/> <!-- rosconsole, roslisp, std_srvs, roscpp, roslib, std_msgs, rospy -->
  <depend stack="navigation"/> <!-- move_base, map_server, costmap_2d, nav_view, amcl, fake_localization, base_local_planner -->
  <depend stack="joystick_drivers"/> <!-- joy -->
  <depend stack="vision"/> <!-- people -->
  <depend stack="common_msgs"/> <!-- nav_msgs, actionlib_msgs, geometry_msgs, diagnostic_msgs -->

</stack>
