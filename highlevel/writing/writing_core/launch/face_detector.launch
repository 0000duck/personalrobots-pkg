<launch>

  <group ns="people">
    <param name="face_detector/do_continuous" type="bool" value="false" />
    <param name="face_detector/do_publish_faces_of_unknown_size" type="bool" value="false" />
    <param name="face_detector/output" type="string" value="none" />
  </group>
  <node pkg="people" type="face_detector" args="$(find people)/face_detection_nodes.txt" name="face_detector" output="screen">
    <param name="stereo_namespace" type="string" value="wide_stereo"/>
  </node>


</launch>
