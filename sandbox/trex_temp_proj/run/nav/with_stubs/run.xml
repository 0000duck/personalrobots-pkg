<launch>
  <master auto="start"/>
  <param name="/trex/enable_base_state_publisher" value="true"/>
  <param name="/trex/enable_switch_controllers" value="true"/>
  <param name="/trex/enable_move_base" value="true"/>
  <node pkg="trex_temp_proj" type="stub_ros_container" respawn="false" />


  <param name="/trex/input_file" value="driver.cfg"/>
  <param name="/trex/path" value="$(find trex_temp_proj)/cfg:$(find trex_ros)/cfg"/>
  <param name="/trex/start_dir" value="$(find trex_temp_proj)/run/nav/with_stubs"/>
  <param name="/trex/time_limit" value="60"/>
  <param name="/trex/log_dir" value="$(find trex_temp_proj)/logs"/>
  <param name="/trex/play_back" value="0"/>
  <node pkg="trex_temp_proj" type="trexdebug"/>
</launch>
