<launch>

<!-- Gripper effort controller -->
  <param name="r_gripper_effort_controller/autostart" type="bool" value="false" />
  <node pkg="mechanism_control" type="spawner.py" args="$(find pr2_default_controllers)/r_gripper_effort_controller.xml" />
 
<!-- Gripper position controller -->
  <param name="r_gripper_position_controller/autostart" type="bool" value="false" />
  <node pkg="mechanism_control" type="spawner.py" args="$(find pr2_default_controllers)/r_gripper_position_controller.xml" />

</launch>
