<launch>
<param name="base_local_planner/costmap/update_frequency" value="5.0" />
<param name="base_local_planner/costmap/publish_frequency" value="2.0" />

<param name="base_local_planner/costmap/observation_topics" value="base_scan_marking base_scan_clearing tilt_scan_filtered ground_object_cloud" />

<param name="base_local_planner/costmap/base_scan_marking/sensor_frame" value="base_laser" />
<param name="base_local_planner/costmap/base_scan_marking/observation_persistance" value="0.0" />
<param name="base_local_planner/costmap/base_scan_marking/expected_update_rate" value="0.2" />
<param name="base_local_planner/costmap/base_scan_marking/data_type" value="PointCloud" />
<param name="base_local_planner/costmap/base_scan_marking/marking" value="true" />
<param name="base_local_planner/costmap/base_scan_marking/clearing" value="false" />
<param name="base_local_planner/costmap/base_scan_marking/min_obstacle_height" value="0.08" />
<param name="base_local_planner/costmap/base_scan_marking/max_obstacle_height" value="2.0" />

<param name="base_local_planner/costmap/base_scan_clearing/sensor_frame" value="base_laser" />
<param name="base_local_planner/costmap/base_scan_clearing/observation_persistance" value="0.0" />
<param name="base_local_planner/costmap/base_scan_clearing/expected_update_rate" value="0.2" />
<param name="base_local_planner/costmap/base_scan_clearing/data_type" value="PointCloud" />
<param name="base_local_planner/costmap/base_scan_clearing/marking" value="false" />
<param name="base_local_planner/costmap/base_scan_clearing/clearing" value="true" />
<param name="base_local_planner/costmap/base_scan_clearing/min_obstacle_height" value="0.10" />
<param name="base_local_planner/costmap/base_scan_clearing/max_obstacle_height" value="2.0" />

<param name="base_local_planner/costmap/tilt_scan_filtered/sensor_frame" value="frame_from_message" />
<param name="base_local_planner/costmap/tilt_scan_filtered/observation_persistance" value="2.0" />
<param name="base_local_planner/costmap/tilt_scan_filtered/expected_update_rate" value="0.2" />
<param name="base_local_planner/costmap/tilt_scan_filtered/data_type" value="PointCloud" />
<param name="base_local_planner/costmap/tilt_scan_filtered/clearing" value="false" />
<param name="base_local_planner/costmap/tilt_scan_filtered/min_obstacle_height" value="0.08" />
<param name="base_local_planner/costmap/tilt_scan_filtered/max_obstacle_height" value="2.0" />

<param name="base_local_planner/costmap/ground_object_cloud/sensor_frame" value="frame_from_message" />
<param name="base_local_planner/costmap/ground_object_cloud/observation_persistance" value="2.0" />
<param name="base_local_planner/costmap/ground_object_cloud/expected_update_rate" value="0.2" />
<param name="base_local_planner/costmap/ground_object_cloud/data_type" value="PointCloud" />
<param name="base_local_planner/costmap/ground_object_cloud/clearing" value="false" />
<param name="base_local_planner/costmap/ground_object_cloud/min_obstacle_height" value="-0.10" />
<param name="base_local_planner/costmap/ground_object_cloud/max_obstacle_height" value="2.0" />

<param name="base_local_planner/costmap/static_map" value="false" />
<param name="base_local_planner/costmap/rolling_window" value="true" />

<param name="base_local_planner/costmap/width" value="6.0" />
<param name="base_local_planner/costmap/height" value="6.0" />
<param name="base_local_planner/costmap/resolution" value="0.025" />
<param name="base_local_planner/costmap/origin_x" value="-5.0" />
<param name="base_local_planner/costmap/origin_y" value="-5.0" />

<param name="base_local_planner/acc_limit_x" value="2.5" />
<param name="base_local_planner/acc_limit_y" value="2.5" />
<param name="base_local_planner/acc_limit_th" value="3.2" />
<param name="base_local_planner/sim_time" value="1.7" />
<param name="base_local_planner/sim_granularity" value="0.05" />
<param name="base_local_planner/vx_samples" value="3" />
<param name="base_local_planner/vtheta_samples" value="20" />
<param name="base_local_planner/path_distance_bias" value="0.6" />
<param name="base_local_planner/goal_distance_bias" value="0.8" />
<param name="base_local_planner/occdist_scale" value="0.05" />
<param name="base_local_planner/heading_lookahead" value="0.325" />
<param name="base_local_planner/oscillation_reset_dist" value="0.05" />
<param name="base_local_planner/holonomic_robot" value="true" />
<param name="base_local_planner/max_vel_x" value="0.65" />
<param name="base_local_planner/min_vel_x" value="0.1" />
<param name="base_local_planner/max_vel_th" value="1.0" />
<param name="base_local_planner/min_vel_th" value="-1.0" />
<param name="base_local_planner/min_in_place_vel_th" value="0.4" />
<param name="base_local_planner/world_model" value="freespace" />
<param name="base_local_planner/dwa" value="true" />
<param name="base_local_planner/heading_scoring" value="false" />
<param name="base_local_planner/heading_scoring_timestep" value="0.8" />
<param name="base_local_planner/simple_attractor" value="false" />

<param name="base_local_planner/yaw_goal_tolerance" value=".05" />
<param name="base_local_planner/xy_goal_tolerance" value=".1" />
</launch>
